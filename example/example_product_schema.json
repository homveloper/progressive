{
  "type": "object",
  "title": "상품 정보",
  "description": "온라인 쇼핑몰 상품 스키마",
  "properties": {
    "productId": {
      "type": "string",
      "description": "상품 ID"
    },
    "productName": {
      "type": "string",
      "description": "상품명"
    },
    "category": {
      "type": "string",
      "description": "카테고리",
      "enum": ["전자제품", "의류", "식품", "도서", "가구", "기타"]
    },
    "price": {
      "type": "integer",
      "minimum": 0,
      "description": "가격 (원)"
    },
    "discount": {
      "type": "number",
      "minimum": 0,
      "maximum": 100,
      "description": "할인율 (%)"
    },
    "stock": {
      "type": "integer",
      "minimum": 0,
      "description": "재고 수량"
    },
    "description": {
      "type": "string",
      "description": "상품 설명"
    },
    "manufacturer": {
      "type": "string",
      "description": "제조사"
    },
    "inStock": {
      "type": "boolean",
      "description": "재고 여부"
    },
    "rating": {
      "type": "number",
      "minimum": 0,
      "maximum": 5,
      "description": "평점"
    }
  },
  "required": ["productId", "productName", "price"]
}